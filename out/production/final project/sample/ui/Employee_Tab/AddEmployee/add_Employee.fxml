<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="593.0" prefWidth="506.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" >
    <children>
        <VBox prefHeight="577.0" prefWidth="506.0">
            <children>
                <JFXTextField fx:id="first_name" labelFloat="true" prefHeight="41.0" prefWidth="580.0" promptText="First Name">
                    <VBox.margin>
                        <Insets left="10.0" right="10.0" top="20.0" />
                    </VBox.margin>
                </JFXTextField>
                <JFXTextField fx:id="last_name" labelFloat="true" layoutX="20.0" layoutY="30.0" promptText="Last Name">
                    <VBox.margin>
                        <Insets left="10.0" right="10.0" top="20.0" />
                    </VBox.margin>
                </JFXTextField>
                <JFXTextField fx:id="id" labelFloat="true" layoutX="20.0" layoutY="30.0" prefHeight="38.0" prefWidth="562.0" promptText="ID">
                    <VBox.margin>
                        <Insets left="10.0" right="10.0" top="20.0" />
                    </VBox.margin>
                </JFXTextField>
                <JFXTextField fx:id="ssn" labelFloat="true" prefHeight="38.0" prefWidth="562.0" promptText="SSN">
                    <VBox.margin>
                        <Insets left="10.0" right="10.0" top="20.0" />
                    </VBox.margin>
                </JFXTextField>
                <JFXTextField fx:id="salary" labelFloat="true" layoutX="20.0" layoutY="110.0" prefHeight="38.0" prefWidth="600.0" promptText="Salary">
                    <VBox.margin>
                        <Insets left="10.0" right="10.0" top="20.0" />
                    </VBox.margin>
                </JFXTextField>
                <JFXTextField fx:id="age" labelFloat="true" layoutX="10.0" layoutY="145.0" prefHeight="42.0" prefWidth="600.0" promptText="Age">
                    <VBox.margin>
                        <Insets left="10.0" right="10.0" top="20.0" />
                    </VBox.margin>
                </JFXTextField>
                <JFXTextField fx:id="job" labelFloat="true" layoutX="10.0" layoutY="217.0" prefHeight="48.0" prefWidth="600.0" promptText="Job">
                    <VBox.margin>
                        <Insets left="10.0" right="10.0" top="20.0" />
                    </VBox.margin>
                </JFXTextField>
                <JFXTextField fx:id="department_id" labelFloat="true" layoutX="10.0" layoutY="215.0" prefHeight="41.0" prefWidth="506.0" promptText="Department ID">
                    <VBox.margin>
                        <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
                    </VBox.margin>
                </JFXTextField>
                <JFXTextField fx:id="address" labelFloat="true" layoutX="20.0" layoutY="357.0" prefHeight="41.0" prefWidth="506.0" promptText="Address">
                    <VBox.margin>
                        <Insets left="10.0" right="10.0" top="20.0" />
                    </VBox.margin>
                </JFXTextField>
                <JFXTextField fx:id="phone" labelFloat="true" layoutX="20.0" layoutY="410.0" prefHeight="41.0" prefWidth="506.0" promptText="Phone">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
                    </VBox.margin>
                </JFXTextField>
                <JFXTextField fx:id="birth_day" labelFloat="true" layoutX="20.0" layoutY="494.0" prefHeight="41.0" prefWidth="506.0" promptText="birth day">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
                    </VBox.margin>
                </JFXTextField>
                <HBox prefHeight="65.0" prefWidth="488.0">
                    <children>
                        <JFXButton fx:id="saveButton"  prefHeight="42.0" prefWidth="243.0" stylesheets="@addEmployee.css" text="Save">
                            <HBox.margin>
                                <Insets />
                            </HBox.margin>
                        </JFXButton>
                        <JFXButton fx:id="cancelButton" layoutX="10.0" layoutY="10.0"  prefHeight="42.0" prefWidth="263.0" stylesheets="@addEmployee.css" text="Cancel" />
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
